-module(other).                            |         |
                                           |         |
-compile([export_all, nowarn_export_all]). |         |
                                           |         |
% occurrence typing                        |         |
-spec clauses(term()) ->                   |         |
    atom() | number().                     |         |
clauses(X) ->                              | OK      |
    case X of                              |         |
        ok -> X;                           |         |
        1 -> X                             |         |
    end.                                   |         |
                                           |         |
-spec inter                                |         |
    (ok) -> nok;                           |         |
    (nok) -> ok.                           |         |
inter(ok) -> nok;                          | OK      |
inter(nok) -> ok.                          |         |
                                           |         |
-spec slice_inter1(ok) -> nok.             |         |
slice_inter1(ok) -> inter(ok).             | OK      |
                                           |         |
-spec slice_inter2(ok) -> nok.             |         |
slice_inter2(ok) ->                        | OK      |
    other:inter(ok).                       |         |
                                           |         |
-spec get_inter1() ->                      |         |
    term().                                |         |
get_inter1() ->                            | OK      |
    fun inter/1.                           |         |
                                           |         |
-spec get_inter2() ->                      |         |
    term().                                |         |
get_inter2() ->                            | OK      |
    fun other:inter/1.                     |         |
                                           |         |
% logger:metadata() is automatically       |         |
% approximated                             |         |
-spec any_as_metadata_neg(term()) ->       |         |
    logger:metadata().                     |         |
any_as_metadata_neg(M) -> M.               | ERROR   | M.
                                           |         | Expression has type:   term()
                                           |         | Context expected type: logger:metadata()
                                           |         | 
                                           |         |   term() is not compatible with logger:metadata()
                                           |         |   because
                                           |         |   term() is not compatible with #D{dynamic() => dynamic()}
                                           |         |
-spec elab_var_scope(term()) -> term().    |         |
elab_var_scope(_) ->                       | OK      |
    (begin X = 3, X, fun () -> ok end end)……         |
    X.                                     |         |
