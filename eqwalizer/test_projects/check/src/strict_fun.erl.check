-module(strict_fun).                       |         |
-compile([export_all, nowarn_export_all]). |         |
                                           |         |
-record(fun_wrap1, {                       |         |
  inner_fun :: fun()                       |         |
}).                                        |         |
                                           |         |
-record(fun_wrap2, {                       |         |
  inner_fun :: fun((atom()) -> atom())     |         |
}).                                        |         |
                                           |         |
-spec mk_fun_wrap1a() -> #fun_wrap1{}.     |         |
mk_fun_wrap1a() ->                         | OK      |
  #fun_wrap1{inner_fun =                   |         |
    fun mk_fun_wrap1a/0                    |         |
  }.                                       |         |
                                           |         |
-spec mk_fun_wrap1b() -> #fun_wrap1{}.     |         |
mk_fun_wrap1b() ->                         | OK      |
  #fun_wrap1{inner_fun =                   |         |
    fun lists:filter/2                     |         |
  }.                                       |         |
                                           |         |
-spec mk_fun_wrap2(fun()) ->               |         |
  #fun_wrap2{}.                            |         |
mk_fun_wrap2(F) ->                         | OK      |
  #fun_wrap2{inner_fun = F}.               |         |
                                           |         |
-spec filter_any(fun(), list()) -> list(). |         |
filter_any(F, L) ->                        | OK      |
  lists:filter(F, L).                      |         |
                                           |         |
-spec fun_apply(fun(), atom()) -> atom().  |         |
fun_apply(F, A) ->                         | OK      |
  F(A).                                    |         |
