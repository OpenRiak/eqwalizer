-spec unzip1_neg([{integer(),atom()}])     |         |
        -> {[atom()], [integer()]}.        |         |
unzip1_neg([]) -> {[],[]};                 | ERROR   |
unzip1_neg([{H1, H2}|T]) ->                |         |
    {T1, T2} = unzip1_neg(T),              |         |
    {[H1|T1], [H2|T2]}.                    |         | {[H1 | T1], [H2 | T2]}.
                                           |         | Expected: {[atom()], [number()]}
                                           |         | Got     : {[number() | atom()], [atom() | number()]}
                                           |         | 
                                           |         |   at tuple index 1:
                                           |         |   {[number() | atom()], [atom() | number()]} is not a subtype of {[atom()], [number()]}
                                           |         |   because
                                           |         |   [number() | atom()] is not a subtype of [atom()]
                                           |         |   because
                                           |         |   number() | atom() is not a subtype of atom()
                                           |         |   because
                                           |         |   number() is not a subtype of atom()
                                           |         |
-spec unzip2_neg([{integer(),atom()}])     |         |
    -> {[atom()] | [integer()],            |         |
        [atom()] | [integer()]}.           |         |
unzip2_neg([]) -> {[],[]};                 | ERROR   |
unzip2_neg([{H1, H2}|T]) ->                |         |
    {T1, T2} = unzip2_neg(T),              |         |
    {[H1|T1], [H2|T2]}.                    |         | {[H1 | T1], [H2 | T2]}.
                                           |         | Expected: {[atom()] | [number()], [atom()] | [number()]}
                                           |         | Got     : {[atom() | number()], [atom() | number()]}
                                           |         | 
                                           |         |   at tuple index 1:
                                           |         |   {[atom() | number()], [atom() | number()]} is not a subtype of {[atom()] | [number()], [atom()] | [number()]}
                                           |         |   because
                                           |         |   [atom() | number()] is not a subtype of [atom()] | [number()]
                                           |         |   because
                                           |         |   [atom() | number()] is not a subtype of [atom()]
                                           |         |   because
                                           |         |   atom() | number() is not a subtype of atom()
                                           |         |
