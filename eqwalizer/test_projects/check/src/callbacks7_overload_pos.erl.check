-module(callbacks7_overload_pos).          |         |
                                           |         |
-export([                                  |         |
  init/1,                                  |         |
  handle_call/3,                           |         |
  handle_cast/2                            |         |
]).                                        |         |
-behavior(gen_server).                     |         |
                                           |         |
init([]) -> {ok, []}.                      | OK      |
-spec handle_call                          |         |
    ({a, atom()}, term(), term())          |         |
      -> {reply, term(), term()};          |         |
    ({n, number()}, term(), term())        |         |
      -> {reply, term(), term()}.          |         |
handle_call({a, A}, From, State) ->        | OK      |
  log_atom(A),                             |         |
  {reply, From, State};                    |         |
handle_call({n, N}, From, State) ->        |         |
  log_number(N),                           |         |
  {reply, From, State}.                    |         |
                                           |         |
-spec handle_cast(ok, ok) ->               |         |
  {stop, ok, ok}.                          |         |
handle_cast(_, _) ->                       | OK      |
  {stop, ok, ok}.                          |         |
                                           |         |
-spec log_atom(atom()) -> ok.              |         |
log_atom(_A) -> ok.                        | OK      |
                                           |         |
-spec log_number(number()) -> ok.          |         |
log_number(_N) -> ok.                      | OK      |
