-spec lam_arg_1_pos() -> [n()].            |         |
lam_arg_1_pos() ->                         | OK      |
    lmap_n_to_n(                           |         |
        fun(X) -> X * 2 end,               |         |
        [1,2,3]                            |         |
    ).                                     |         |
                                           |         |
-spec lam_arg_2_neg() -> [n()].            |         |
lam_arg_2_neg() ->                         | ERROR   |
    lmap_n_to_n(fun(X) ->                  |         |
        X ++ [] end,                       |         | X.
                                           |         | Expected: [term()]
                                           |         | Got     : n()
                                           |         | 
                                           |         |   n() is not a subtype of [term()]
                                           |         |   because
                                           |         |   number() is not a subtype of [term()]
        [1,2,3]                            |         |
    ).                                     |         |
                                           |         |
-spec lam_arg_3_neg() -> [n()].            |         |
lam_arg_3_neg() ->                         | ERROR   |
    lmap_n_to_n(fun(X) ->                  |         |
            erlang:atom_to_binary(X)       |         | X.
                                           |         | Expected: atom()
                                           |         | Got     : n()
                                           |         | 
                                           |         |   n() is not a subtype of atom()
                                           |         |   because
                                           |         |   number() is not a subtype of atom()
            end,                           |         |
        [1,2,3]                            |         |
    ).                                     |         |
                                           |         |
-spec lam_arg_4_neg() -> [a].              |         |
lam_arg_4_neg() ->                         | ERROR   |
    lmap_n_to_n(                           |         | lmap_n_to_n(fun, [1, 2, 3]).
                                           |         | Expected: ['a']
                                           |         | Got     : [n()]
                                           |         | 
                                           |         |   [n()] is not a subtype of ['a']
                                           |         |   because
                                           |         |   n() is not a subtype of 'a'
                                           |         |   because
                                           |         |   number() is not a subtype of 'a'
        fun(X) -> X end,                   |         |
        [1,2,3]                            |         |
    ).                                     |         |
                                           |         |
