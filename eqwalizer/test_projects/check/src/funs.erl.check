-type int_result() :: fun(() ->            |         |
    {ok, integer()} | error).              |         |
                                           |         |
-spec ints([int_result()]) ->              |         |
    [integer()].                           |         |
ints([]) -> [];                            | OK      |
ints([F | Fs]) ->                          |         |
    case F() of                            |         |
        {ok, I} -> [I | ints(Fs)];         |         |
        error -> ints(Fs)                  |         |
    end.                                   |         |
                                           |         |
-spec lambda_app() -> number().            |         |
lambda_app() ->                            | ERROR   |
    Res = (fun                             |         |
        Fib(N) when N < 2 ->               |         |
            1;                             |         |
        Fib(N) ->                          |         |
            Fib(N - 2) + Fib(N - 1)        |         | Unbound var: Fib

    end)(4),                               |         |
    Res.                                   |         |
                                           |         |
