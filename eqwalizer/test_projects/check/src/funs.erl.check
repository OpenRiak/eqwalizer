-type stream(A) :: eos | {head, A}.        |         |
                                           |         |
-spec id_predicate(fun((stream(A)) -> ok))……         |
id_predicate(P) -> P.                      | OK      |
                                           |         |
-spec wrap_id(fun((stream(A)) -> ok)) -> (……         |
wrap_id(P) -> id_predicate(P).             | ERROR   | P.
                                           |         | Expected: fun((stream(A)) -> 'ok')
                                           |         | Got     : fun((stream(A)) -> 'ok')
                                           |         | 
                                           |         |   fun((stream(A)) -> 'ok') is not a subtype of fun((stream(A)) -> 'ok')
                                           |         |   because
                                           |         |   stream(A) is not a subtype of stream(A)
                                           |         |   because
                                           |         |   'eos' | {'head', A} is not a subtype of stream(A)
                                           |         |   because
                                           |         |   'eos' | {'head', A} is not a subtype of 'eos' | {'head', A}
