                                           |         | Context expected type: f4('a' | 'b')
                                           |         | 
                                           |         |   f4('a') is not compatible with f4('a' | 'b')
                                           |         |   because
                                           |         |   fun((...) -> 'a') is not compatible with f4('a' | 'b')
                                           |         |   because
                                           |         |   fun((...) -> 'a') is not compatible with fun((...) -> 'a' | 'b')
                                           |         |
