% fun() is like f0() | f1() | f3() ...     |         |
-type f0() :: fun(() -> term()).           |         |
-type f1() :: fun((term()) -> term()).     |         |
-type f2() ::                              |         |
    fun((term(), term()) -> term()).       |         |
-type f3() ::                              |         |
    fun((term(), term(), term()) -> term()……         |
                                           |         |
-spec f0_to_f_any_pos(f0()) -> fun().      |         |
f0_to_f_any_pos(F) -> F.                   | OK      |
                                           |         |
-spec f1_to_f_any_pos(f1()) -> fun().      |         |
f1_to_f_any_pos(F) -> F.                   | OK      |
