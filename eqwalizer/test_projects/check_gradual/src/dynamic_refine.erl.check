filter_none(D) ->                          | ERROR   |
    maps:filtermap(                        |         |
        fun(_, X) ->                       |         | fun.
                                           |         | Expected: fun((Key, Value1) -> boolean() | {'true', Value2})
                                           |         | Got     : fun((none(), none()) -> 'false' | {'true', none()})
                                           |         | 
                                           |         |   fun((none(), none()) -> 'false' | {'true', none()}) is not a subtype of fun((Key, Value1) -> boolean() | {'true', Value2})
                                           |         |   because
                                           |         |   Key is not a subtype of none()
            case X of                      |         |
                none -> false;             |         |
                _ -> {true, X}             |         |
            end                            |         |
        end,                               |         |
        D                                  |         |
    ).                                     |         |
