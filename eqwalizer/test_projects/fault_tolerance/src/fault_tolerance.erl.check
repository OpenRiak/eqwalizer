-spec comprehensions1() -> ok.             |         |
comprehensions1() ->                       | ERROR   |
  [X ||                                    |         | [ || ].
                                           |         | Expected: 'ok'
                                           |         | Got     : [dynamic()]
    X <- #{                                |         | #{..}.
                                           |         | Expected: [term()]
                                           |         | Got     : #S{a := 'b', c := number()}
      a => b,                              |         |
      c => d + 1                           |         | 'd'.
                                           |         | Expected: number()
                                           |         | Got     : 'd'
    },                                     |         |
    atom_to_binary(                        |         |
      {X}                                  |         | {X}.
                                           |         | Expected: atom()
                                           |         | Got     : {dynamic()}
    )].                                    |         |
                                           |         |
-spec comprehensions2([any()]) -> ok.      |         |
comprehensions2(L) ->                      | ERROR   |
  Res = [                                  |         |
    atom_to_binary(Y) ||                   |         | Y.
                                           |         | Expected: atom()
                                           |         | Got     : number()
    <<Y>> <= L                             |         | L.
                                           |         | Expected: binary()
                                           |         | Got     : [term()]
  ],                                       |         |
  Res.                                     |         | Res.
                                           |         | Expected: 'ok'
                                           |         | Got     : [binary()]
                                           |         |
-spec comprehensions3(binary()) -> ok.     |         |
comprehensions3(B) ->                      | ERROR   |
  Res =                                    |         |
    <<Y || Y <-                            |         |
      B                                    |         | B.
                                           |         | Expected: [term()]
                                           |         | Got     : binary()
    >>,                                    |         |
  Res.                                     |         | Res.
                                           |         | Expected: 'ok'
                                           |         | Got     : binary()
                                           |         |
-spec maps(map()) ->                       |         |
  ok.                                      |         |
maps(M) ->                                 | ERROR   |
  M1 = M                                   |         |
    #{id := a + 1},                        |         | 'a'.
                                           |         | Expected: number()
                                           |         | Got     : 'a'
  M2 = M                                   |         |
    #{id := b + 2},                        |         | 'b'.
                                           |         | Expected: number()
                                           |         | Got     : 'b'
  M1 + M2.                                 |         | M1.
                                           |         | Expected: number()
                                           |         | Got     : #D{dynamic() | 'id' => dynamic() | number()}
                                           |         | ---
                                           |         | M2.
                                           |         | Expected: number()
                                           |         | Got     : #D{dynamic() | 'id' => dynamic() | number()}
                                           |         | ---
                                           |         | _ + _.
                                           |         | Expected: 'ok'
                                           |         | Got     : number()
                                           |         |
-spec binary1(atom(), atom()) -> atom().   |         |
binary1(A, S) ->                           | ERROR   |
  B = <<                                   |         |
    A:                                     |         | A.
                                           |         | Expected: number()
                                           |         | Got     : atom()
    S                                      |         | S.
                                           |         | Expected: number()
                                           |         | Got     : atom()
  >>,                                      |         |
  B + 1.                                   |         | B.
                                           |         | Expected: number()
                                           |         | Got     : binary()
                                           |         | ---
                                           |         | _ + _.
                                           |         | Expected: atom()
                                           |         | Got     : number()
                                           |         |
-spec 'catch'(atom()) -> atom().           |         |
'catch'(F) ->                              | ERROR   |
  Res =                                    |         |
    catch F(),                             |         | F.
                                           |         | Expected fun type with arity 0
                                           |         | Got: atom()
  Res.                                     |         |
                                           |         |
-spec try1(atom()) -> atom().              |         |
try1(A) ->                                 | ERROR   |
  Res = try                                |         |
    A()                                    |         | A.
                                           |         | Expected fun type with arity 0
                                           |         | Got: atom()
  catch _ ->                               |         |
    []                                     |         |
  after                                    |         |
    1 + a                                  |         | 'a'.
                                           |         | Expected: number()
                                           |         | Got     : 'a'
  end,                                     |         |
  Res.                                     |         | Res.
                                           |         | Expected: atom()
                                           |         | Got     : dynamic() | []
                                           |         | 
                                           |         |   dynamic() | [] is not a subtype of atom()
                                           |         |   because
                                           |         |   [] is not a subtype of atom()
                                           |         |
-spec try2(atom()) -> atom().              |         |
try2(A) ->                                 | ERROR   |
  Res = try                                |         |
          A()                              |         | A.
                                           |         | Expected fun type with arity 0
                                           |         | Got: atom()
        of                                 |         |
          {_} ->                           |         |
            A + 1                          |         | A.
                                           |         | Expected: number()
                                           |         | Got     : atom()
        catch _ ->                         |         |
          []                               |         |
        after                              |         |
          1 + a                            |         | 'a'.
                                           |         | Expected: number()
                                           |         | Got     : 'a'
        end,                               |         |
  Res.                                     |         | Res.
                                           |         | Expected: atom()
                                           |         | Got     : number() | []
                                           |         |
