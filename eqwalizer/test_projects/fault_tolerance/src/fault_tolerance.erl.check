rec1(Pid, Id) ->                           | ERROR   |
  Res = #rec{                              |         |
    id = Pid,                              |         | Pid.
                                           |         | Expected: number()
                                           |         | Got     : pid()
    pid = Id                               |         | Id.
                                           |         | Expected: pid()
                                           |         | Got     : number()
  },                                       |         |
  Res.                                     |         | Res.
                                           |         | Expected: atom()
                                           |         | Got     : #rec{}
                                           |         |
-spec rec2(#rec{}, integer()) -> pid().    |         |
rec2(Rec, Id) ->                           | ERROR   |
  Res = Rec#rec{                           |         |
    pid = Id                               |         | Id.
                                           |         | Expected: pid()
                                           |         | Got     : number()
  },                                       |         |
  Res#rec.id.                              |         | ...#rec.id.
                                           |         | Expected: pid()
                                           |         | Got     : number()
