-spec block                                |         |
    (any(), any()) -> atom().              |         |
block(X, Y) ->                             | ERROR   |
  begin                                    |         |
    Z =                                    |         |
      X +                                  |         | X.
                                           |         | Expected: number()
                                           |         | Got     : term()
        Y,                                 |         | Y.
                                           |         | Expected: number()
                                           |         | Got     : term()
    Z                                      |         | Z.
                                           |         | Expected: atom()
                                           |         | Got     : number()
  end.                                     |         |
                                           |         |
-spec cons                                 |         |
    (any(), any()) -> [atom()].            |         |
cons(H, T) ->                              | ERROR   |
  Res = lists:append(                      |         |
    [H |                                   |         | T.
                                           |         | Expected: [term()]
                                           |         | Got     : term()
      T],                                  |         |
    [T |                                   |         | H.
                                           |         | Expected: [term()]
                                           |         | Got     : term()
      H]                                   |         |
  ),                                       |         |
  Res.                                     |         | Res.
                                           |         | Expected: [atom()]
                                           |         | Got     : [term()]
                                           |         | 
                                           |         |   [term()] is not a subtype of [atom()]
                                           |         |   because
                                           |         |   term() is not a subtype of atom()
                                           |         |
-spec 'case'                               |         |
    (boolean(), atom()) -> number().       |         |
'case'(F, A) ->                            | ERROR   |
  case F() of                              |         | F.
                                           |         | Expected fun type with arity 0
                                           |         | Got: boolean()
    true -> A + 1;                         |         | A.
                                           |         | Expected: number()
                                           |         | Got     : atom()
    false -> A - 1                         |         | A.
                                           |         | Expected: number()
                                           |         | Got     : atom()
  end.                                     |         |
                                           |         |
-spec 'if'                                 |         |
    (boolean(), atom()) -> number().       |         |
'if'(F, A) ->                              | ERROR   |
  if                                       |         |
    F -> A + 1;                            |         | A.
                                           |         | Expected: number()
                                           |         | Got     : atom()
    true -> A - 1                          |         | A.
