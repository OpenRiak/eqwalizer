-spec b_to_n(boolean()) ->                 |         |
  number().                                |         |
b_to_n(false) ->                           | OK      |
  0;                                       |         |
b_to_n(true) ->                            |         |
  1.                                       |         |
                                           |         |
-spec b_to_n1(boolean()) ->                |         |
  number().                                |         |
b_to_n1(true) ->                           | ERROR   |
  true;                                    |         | 'true'.
                                           |         | Expected: number()
                                           |         | Got     : 'true'
b_to_n1(false) ->                          |         |
  false.                                   |         | 'false'.
                                           |         | Expected: number()
                                           |         | Got     : 'false'
                                           |         |
-spec b_to_n2(boolean()) ->                |         |
  number().                                |         |
b_to_n2(true) ->                           | ERROR   |
  1;                                       |         |
b_to_n2(A) ->                              |         |
  A.                                       |         | A.
                                           |         | Expected: number()
                                           |         | Got     : 'false' | none()
                                           |         | 
                                           |         |   'false' | none() is not a subtype of number()
                                           |         |   because
                                           |         |   'false' is not a subtype of number()
                                           |         |
-spec b_to_n3(boolean()) ->                |         |
  number().                                |         |
b_to_n3(true) ->                           | ERROR   |
  true;                                    |         | 'true'.
                                           |         | Expected: number()
                                           |         | Got     : 'true'
b_to_n3(A) ->                              |         |
  A.                                       |         | A.
                                           |         | Expected: number()
                                           |         | Got     : 'false' | none()
                                           |         | 
                                           |         |   'false' | none() is not a subtype of number()
                                           |         |   because
                                           |         |   'false' is not a subtype of number()
                                           |         |
