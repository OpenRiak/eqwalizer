-spec receive1()                           |         |
      -> number().                         |         |
receive1() ->                              | ERROR   |
  receive                                  |         |
    {N} ->                                 |         |
      atom_to_binary(                      |         | erlang:atom_to_binary(eqwalizer:dynamic_cast(_ ++ _)).
                                           |         | Expected: number()
                                           |         | Got     : binary()
        [N]                                |         | _ ++ _.
                                           |         | Expected: atom()
                                           |         | Got     : [dynamic()]
          ++ a                             |         | 'a'.
                                           |         | Expected: [term()]
                                           |         | Got     : 'a'
      );                                   |         |
    N ->                                   |         |
      atom_to_binary(                      |         |
        <<N>>                              |         | <<..>>.
                                           |         | Expected: [term()]
                                           |         | Got     : binary()
          ++ b                             |         | 'b'.
                                           |         | Expected: [term()]
                                           |         | Got     : 'b'
      )                                    |         |
  end.                                     |         |
                                           |         |
-spec receive2()                           |         |
      -> number().                         |         |
receive2() ->                              | ERROR   |
  receive                                  |         |
    {N} ->                                 |         |
      atom_to_binary(                      |         | erlang:atom_to_binary(eqwalizer:dynamic_cast([N])).
                                           |         | Expected: number()
                                           |         | Got     : binary()
        [N]                                |         | [N].
                                           |         | Expected: atom()
                                           |         | Got     : [dynamic()]
      );                                   |         |
    N ->                                   |         |
      atom_to_binary(                      |         | erlang:atom_to_binary(eqwalizer:dynamic_cast(<<..>>)).
                                           |         | Expected: number()
                                           |         | Got     : binary()
        <<N>>                              |         | <<..>>.
                                           |         | Expected: atom()
                                           |         | Got     : binary()
      )                                    |         |
  after 10 ->                              |         |
    []                                     |         | [].
                                           |         | Expected: number()
                                           |         | Got     : []
  end.                                     |         |
                                           |         |
